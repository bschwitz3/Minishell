${NAME}:	${OBJS}
			${MAKE} -C ./libft
			${CC} ${CFLAGS} \
			./libft/libft.a \
			${OBJS} \
			${INCLUDES_READLINE} \
			${LINK_READLINE} \
			-o ${NAME}

norm:
			norminette ${SRCS} minishell.h
clean:
			${MAKE} -C ./libft clean
			${RM} ${OBJS}

fclean:		clean
			${MAKE} -C ./libft fclean
			${RM} ${NAME}

re:			fclean all

.PHONY:		all clean fclean re
